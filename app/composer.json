{
    "name": "asparatu/docker-php",
    "description": "Docker PHP Development Environment",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Shane Saunders",
            "email": "asparatu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "psr/cache": "1.0.1",
        "psr/log": "1.1.4",
        "psr/container": "1.1.0",
        "symfony/deprecation-contracts": "2.5.2",
        "symfony/cache-contracts": "2.5.2",
        "symfony/service-contracts": "2.5.2",
        "symfony/var-exporter": "5.4.26",
        "symfony/cache": "5.4.28",
        "predis/predis": "2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "scripts": {
        "run-phpunit": "vendor/bin/phpunit"
    }
}
